{
  "name": "my-personal-page",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "cross-env NEXT_TURBO=1 next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "export": "next build",
    "create-nojekyll": "node -e \"const fs=require('fs');const dir='out';if(!fs.existsSync(dir)){fs.mkdirSync(dir);}fs.writeFileSync('out/.nojekyll', '')\"",
    "predeploy": "npm run export && npm run create-nojekyll",
    "deploy": "node scripts/deploy.js",
    "build-and-deploy": "npm run predeploy && npm run deploy"
  },
  "dependencies": {
    "apexcharts": "^4.7.0",
    "chart.js": "^4.4.1",
    "firebase": "^11.6.0",
    "framer-motion": "^12.6.5",
    "google-auth-library": "^9.15.1",
    "googleapis": "^148.0.0",
    "katex": "^0.16.22",
    "leaflet": "^1.9.4",
    "next": "15.3.0",
    "react": "^19.0.0",
    "react-apexcharts": "^1.7.0",
    "react-chartjs-2": "^5.2.0",
    "react-dom": "^19.0.0",
    "react-icon": "^1.0.0",
    "react-icons": "^5.5.0",
    "react-leaflet": "^5.0.0",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/leaflet": "^1.9.17",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/uuid": "^10.0.0",
    "cross-env": "^7.0.3",
    "eslint": "^9",
    "eslint-config-next": "15.3.0",
    "gh-pages": "^6.3.0",
    "tailwindcss": "^4",
    "typescript": "^5"
  }
}
